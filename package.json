{
  "name": "angular4-cli-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron-build": "ng build -aot -op .\\electron\\app",
    "electron-copyfiles": "copyfiles -u 1 .\\electron\\main.js .\\electron\\package.json .\\electron\\app && copyfiles .\\src\\res\\* .\\electron\\app",
    "electron-run": "cd .\\electron\\app && electron .",
    "electron-serve": "npm run electron-build && npm run electron-copyfiles && npm run electron-run", 
    "cordova-build": "ng build -aot --output-path cordova/www/ --base-href \".\"", 
    "cordova-build-prod": "ng build --target=production --environment=prod --output-path cordova/www/ --base-href \".\"", 
    "cordova-run-android": "npm run cordova-build && copyfiles .\\src\\res\\* .\\cordova\\www && cd cordova && cordova run android", 
    "cordova-run-browser": "npm run cordova-build && copyfiles .\\src\\res\\* .\\cordova\\www && cd cordova && cordova run browser", 
    "cordova-serve": "npm run cordova-build && copyfiles .\\src\\res\\* .\\cordova\\www && cd cordova && cordova serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.3",
    "@angular/common": "^4.0.3",
    "@angular/compiler": "^4.0.3",
    "@angular/core": "^4.0.3",
    "@angular/forms": "^4.0.3",
    "@angular/http": "^4.0.3",
    "@angular/platform-browser": "^4.0.3",
    "@angular/platform-browser-dynamic": "^4.0.3",
    "@angular/router": "^4.0.3",
    "@types/core-js": "^0.9.41",
    "angular2-notifications": "^0.4.53",
    "core-js": "^2.4.1",
    "jquery": "^3.2.1",
    "jquery-ui": "^1.12.1",
    "ngx-bootstrap": "^1.6.6",
    "primeng": "^4.0.0-rc.2",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/jquery": "^2.0.41",
    "@types/jqueryui": "^1.11.32",
    "@types/node": "~6.0.60",
    "bootstrap-sass": "^3.3.7",
    "codelyzer": "~2.0.0",
    "copyfiles": "1.2.0",
    "cordova": "~6.3.1",
    "electron": "1.6.6",
    "electron-reload": "^1.1.0",
    "font-awesome": "^4.7.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.5.2",
    "protractor": "~5.1.0",
    "rcedit": "0.8.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  }
}
